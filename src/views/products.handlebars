{{#if isValid}}
<div>
    {{#each docs}}
    <div>
        <p>Titulo: {{this.title}} &nbsp; Precio: {{this.price}}</p>
    </div>
    <br/>
    {{/each}}
</div>
<br />
<div>
    {{#if hasPrevPage}}
    <a href={{this.prevLink}}>
        << Anterior
    </a>
    {{/if}}
    $nbsp;
    {{this.page}}
    $nbsp;
    {{#if hasNextPage}}
    <a href={{this.nextLink}}>
        Siguiente >>
    </a>
    {{/if}}
</div>
{{else}}
<h1>No hay productos</h1>
{{/if}}
