{{#if totalPages}}
<div>
    {{#each docs}}
    <div>
        <img src="{{this.thumbnails}}" alt="{{this.title}}" style="width: 200px;height: auto;">
        <h1><a href="/{{this._id}}">{{this.title}}</a></h1>
        <p>{{this.category}}</p>
        <h2>{{this.price}}</h2>
    </div>
    <br/>
    {{/each}}
</div>
<br />
<div>
    {{#if prevLink}}
    <a href="{{prevLink}}">
        << Anterior
    </a>
    {{/if}}
    &nbsp;
    {{this.page}}
    &nbsp;
    {{#if nextLink}}
    <a href="{{nextLink}}">
        Siguiente >>
    </a>
    {{/if}}
</div>
{{else}}
<h1>No hay productos</h1>
{{/if}}

